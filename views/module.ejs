<style>
    table, th, td { border: 1px solid black }
</style>
<h1>Modules</h1><br><br>
<table>
    <tr>
        <th>Module ID </th>
        <th>Name </th>
        <th>Credits </th>
        <th>Update </th>
        <th>Students </th>
    </tr>
    <% modules.forEach((module)=> { %>
        <tr>
            <td>
                <%= module.mid %>
            </td>
            <td>
                <%= module.name %>
            </td>
            <td>
                <%= module.credits %>
            </td>
            <td><a href="http://localhost:3000/module/edit/<%= module.mid %>">Update</a></td>
            <td><a href="http://localhost:3000/module/students/<%= module.mid %>">List</a></td>
        </tr>
        <% }) %>
</table>

<a href="http://localhost:3000/">Home</a> <br>